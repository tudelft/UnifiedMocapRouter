    "version": "0.2.0",
    "configurations": [
    {
        "name": "optitrack2ivy",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/mocap-router",
        "args": ["optitrack", "ivy","-c", "NED", "-r", "far", "-n", "far", "-f", "10", "-s", "125", "--ac_id", "2"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    },
    {
        "name": "test2ros2px4",
